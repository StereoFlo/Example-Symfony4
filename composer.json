{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/DoctrineExtensions": "*",
        "league/tactician-bundle": "^1.1",
        "nelmio/cors-bundle": "^1.5",
        "nesbot/carbon": "^1.27",
        "ramsey/uuid": "^3.7",
        "symfony/console": "*",
        "symfony/flex": "*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/translation": "*",
        "symfony/validator": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/browser-kit": "*",
        "symfony/dotenv": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/web-server-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Fixtures\\": "fixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "copy-params": {
            "copy": "cp config/parameters.yml.dist config/parameters.yml"
        },
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@copy-params",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
